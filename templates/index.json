    "custom_liquid_Nmazzt": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<style>\n   .benefits-section {\n  position: relative;\n  text-align: center;\n  padding: 40px 20px;\n  background-color: #ffffff; \/* Fallback color *\/\n  overflow: hidden; \/* Ensures pseudo-element doesn't overflow *\/\n  z-index: 1; \/* Keeps the section above any background *\/\n}\n\n.benefits-section::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-image: url('https:\/\/cdn.shopify.com\/s\/files\/1\/0865\/6125\/9840\/files\/concept-childrens-health-treatment-childrens-shoes-with-stethoscope-place-text_1.webp?v=1727681152');\n  background-size: cover; \n  background-position: left; \n  background-repeat: no-repeat;\n  z-index: -1; \n  mix-blend-mode: luminosity; \n}\n\n.benefits-section .benefits-wrapper {\n  position: relative;\n  z-index: 2;\n}\n\n.benefits-heading {\n  font-family: Overlock;\n  font-size: 32px;\n  font-weight: 900;\n  line-height: 43px;\n  text-align: center;\n  color: #035c82;\n  margin-bottom: 40px;\n}\n\n\/* Two rows layout for circles *\/\n.benefits-wrapper {\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n}\n\n\/* Rows *\/\n.benefit-row {\n  display: flex;\n  justify-content: center;\n  gap: 20px;\n  flex-wrap: wrap; \/* Allow wrapping for responsiveness *\/\n}\n\n\/* Circle container *\/\n.benefit-circle {\n  width: 150px;\n  height: 150px;\n  background-color: #eaf1eb;\n  border-radius: 50%;\n  border: 2px dashed #1d6148; \/* Dashed border *\/\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  text-align: center;\n  padding: 10px;\n  box-sizing: border-box;\n}\n\n\/* Icon styling *\/\n.benefit-icon {\n  width: 40px;\n  height: 40px;\n  margin-bottom: 10px;\n}\n\n\/* Text inside the circle *\/\n.benefit-circle p {\n  color: #1d6148;\n  font-family: Tahoma;\n  font-size: 14px;\n  font-weight: 400;\n  line-height: 19px;\n  text-align: center;\nmargin: 0;\n}\n\/* Responsiveness for smaller devices *\/\n@media (max-width: 768px) {\n  .benefit-row {\n    flex-direction: row; \/* Maintain row direction *\/\n    justify-content: space-evenly; \/* Spread items evenly *\/\n  }\n\n  .benefit-circle {\n    width: 150px;\n    height: 150px;\n  }\n\n  .benefit-icon {\n    width: 50px;\n    height: 50px;\n  }\n\n  .benefit-circle p {\n    font-size: 14px;\n  }\n}\n\n@media (max-width: 480px) {\n  .benefit-row {\n    flex-direction: row; \/* Maintain row direction *\/\n    justify-content: space-evenly; \/* Spread items evenly *\/\n  }\n\n  .benefit-circle {\n    width: 145px;\n    height: 145px;\n  }\n\n  .benefit-icon {\n    width: 50px;\n    height: 50px;\n  }\n\n  .benefit-circle p {\n    font-size: 12px;\n  }\n}\n\n<\/style>\n\n<div class=\"benefits-section\">\n  <h2 class=\"benefits-heading\">Nurturing Little Ones with Nature's Goodness<\/h2>\n\n  <div class=\"benefits-wrapper\">\n    <!-- First Row with 4 Circles -->\n    <div class=\"benefit-row\">\n      <div class=\"benefit-item\">\n        <div class=\"benefit-circle\">\n          <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0865\/6125\/9840\/files\/manuka.png?v=1727682704\" alt=\"manuka\" class=\"benefit-icon\" \/>\n          <p>India's First Baby Care with Manuka Honey<\/p>\n        <\/div>\n      <\/div>\n\n      <div class=\"benefit-item\">\n        <div class=\"benefit-circle\">\n          <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0865\/6125\/9840\/files\/paediatrician.png?v=1727683010\" alt=\"Paediatrician\" class=\"benefit-icon\" \/>\n          <p>Developed by Paediatrician<\/p>\n        <\/div>\n      <\/div>\n\n      <div class=\"benefit-item\">\n        <div class=\"benefit-circle\">\n          <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0865\/6125\/9840\/files\/baby-face.png?v=1727682703\" alt=\"Dermatologically\" class=\"benefit-icon\" \/>\n          <p>Dermatologically Tested<\/p>\n        <\/div>\n      <\/div>\n\n      \n      <div class=\"benefit-item\">\n        <div class=\"benefit-circle\">\n          <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0865\/6125\/9840\/files\/pipette.png?v=1727682704\" alt=\"Icon 7\" class=\"benefit-icon\" \/>\n          <p>No Artificial Colours or Perfumes<\/p>\n        <\/div>\n      <\/div>\n    <\/div>\n\n    <!-- Second Row with 3 Circles -->\n    <div class=\"benefit-row\">\n      <div class=\"benefit-item\">\n        <div class=\"benefit-circle\">\n          <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0865\/6125\/9840\/files\/leaf.png?v=1727682704\" alt=\"Natural\" class=\"benefit-icon\" \/>\n          <p>Safe, Natural Ingredients<\/p>\n        <\/div>\n      <\/div>\n      \n      <div class=\"benefit-item\">\n        <div class=\"benefit-circle\">\n          <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0865\/6125\/9840\/files\/forbidden.png?v=1727682703\" alt=\"Chemicals\" class=\"benefit-icon\" \/>\n          <p>No Harmful Chemicals<\/p>\n        <\/div>\n      <\/div>\n\n      <div class=\"benefit-item\">\n        <div class=\"benefit-circle\">\n          <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0865\/6125\/9840\/files\/stamp.png?v=1727682703\" alt=\"Icon 6\" class=\"benefit-icon\" \/>\n          <p>FDA Approved<\/p>\n        <\/div>\n      <\/div>\n\n    <\/div>\n  <\/div>\n<\/div>",
